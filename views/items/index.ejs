<!DOCTYPE html>
<html>
	<head>
		<% include ../partials/head.ejs%>
	</head>
	<body>
		<header>
			<% include ../partials/topper.ejs%>
		</header>
		<div id="navbar">
			<% include ../partials/nav.ejs%>
			<li>
				<a href="/items/new">Create a new item</a>
			</li>
		</div>
		<div id="maincontainer">
			<div id="main_sub">
				<a class="make_new" href="/items/new">Create new entry</a>
				<br>
				<h1>All entries by all users:</h1>
				<div class="containerBox">
					<% for(let i = 0; i < items.length; i++){ %>
						<div class="itemBox">
							<div class="displaylist" class="item">
								<a href="/items/<%=items[i]._id%>"><img src="<%=items[i].link%>" alt=""/>
									<%=items[i].name%><br>
								</a>
								<br>
							</div>
						</div>
					<% } %>
				</div>
			</div>
		</div>
		<% include ../partials/footer.ejs%>
	</body>
</html>
